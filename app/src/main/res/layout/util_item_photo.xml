<?xml version="1.0" encoding="utf-8"?>
<bias.zochiwon_suhodae.homemade_guardian_beta_Test_Server.photo.common.widget.SquareItemLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <ImageView
      android:padding="1dip"
      android:layout_gravity="center"
      android:id="@+id/Arrange_Image"
      android:scaleType="centerCrop"
      android:layout_width="match_parent"
      android:background="@drawable/photo_bg"
      android:layout_height="match_parent"
      android:adjustViewBounds="true" />

  <ImageView
      android:layout_margin="5dip"
      android:layout_alignParentTop="true"
      android:layout_alignParentRight="true"
      android:clickable="true"
      android:padding="5dip"
      android:id="@+id/Check_Selected"
      android:src="@drawable/photo_checkbox_bg"
      android:layout_width="30dp"
      android:layout_height="30dp" />
</bias.zochiwon_suhodae.homemade_guardian_beta_Test_Server.photo.common.widget.SquareItemLayout>